{
  "name": "@wpengine/atlas-next",
  "version": "1.0.0-beta.0",
  "description": "Improved Next.js support on WP Engine Atlas",
  "author": "WP Engine",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js",
      "module": "./dist/index.mjs"
    },
    "./cache-handler": {
      "types": "./dist/cache-handler/index.d.ts",
      "require": "./dist/cache-handler/index.js",
      "import": "./dist/cache-handler/index.js",
      "module": "./dist/cache-handler/index.mjs"
    }
  },
  "scripts": {
    "build": "tsup",
    "ci:lint": "eslint ./",
    "ci:test": "nyc --reporter=text --reporter=text-summary npm test",
    "dev": "tsup --watch",
    "lint": "eslint --fix ./",
    "test": "mocha",
    "test:coverage": "nyc --reporter=lcovonly --reporter=html --reporter=text --reporter=text-summary npm test"
  },
  "devDependencies": {
    "@tsconfig/node18": "^18.2.2",
    "@types/chai": "^4.3.11",
    "@types/chai-as-promised": "^7.1.8",
    "@types/mocha": "^10.0.6",
    "@types/node-fetch": "^2.6.9",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.11.0",
    "chai": "^4.3.10",
    "chai-as-promised": "^7.1.1",
    "esbuild-plugin-version-injector": "^1.2.1",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard-with-typescript": "^39.1.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-n": "^16.3.1",
    "eslint-plugin-prettier": "^5.0.1",
    "eslint-plugin-promise": "^6.1.1",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  },
  "peerDependencies": {
    "next": ">=12.2.0 ",
    "node-fetch": ">=2.6.1 <3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
